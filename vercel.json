{
    "version": 2,
    "builds": [
      {
        "src": "/src/*.*",
        "use": "@now/node"
      },
      {
        "src": "/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/src/server.js"
      }
    ]
  }
